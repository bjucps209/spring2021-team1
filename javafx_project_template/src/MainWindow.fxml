<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:controller="MainWindow" xmlns:fx="http://javafx.com/fxml"  stylesheets="@MainWindow.css"
    spacing="10" prefHeight="400" prefWidth="250">

    <Label text="Welcome to public safety escape"/>
    <Button fx:id="btnStart" onAction="#onStartClicked" text="Start"  />
    <Button fx:id="btnHigh" onAction="#onHighClicked" text="Highscores"  />
    <Button fx:id="btnAbout" onAction="#onAboutClicked" text="About"  />
    <Button fx:id="btnHelp" onAction="#onHelpClicked" text="Help"  />

</VBox>